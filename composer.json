{
    "scripts": {
        "test": "phpunit",
        "check": "php-cs-fixer fix --dry-run",
        "fix": "php-cs-fixer fix"
    },
    "require": {
        "symfony/yaml": "^4.1",
        "symfony/console": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">7",
        "friendsofphp/php-cs-fixer": "*"
    },
    "autoload": {
        "psr-4": {
            "dcgen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dcgen\\test\\": "tests/"
        }
    }
}
